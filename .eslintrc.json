{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "extends": [
      ]
    }
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "new-parens": 1,
    "no-consecutive-blank-lines": 0,
    "no-debugger": 1,
    "no-console": 1,
    "curly": 1,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": true,
        "objectsInObjects": true
      }
    ],
    "array-bracket-spacing": [
      "error",
      "never",
      {
        "singleValue": false
      }
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "space-unary-ops": "error",
    "space-infix-ops": "error",
    "key-spacing": [
      "error",
      {
        "mode": "strict",
        "afterColon": true,
        "beforeColon": false
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "space-before-blocks": "error",
    "no-whitespace-before-property": "error"
  }
}